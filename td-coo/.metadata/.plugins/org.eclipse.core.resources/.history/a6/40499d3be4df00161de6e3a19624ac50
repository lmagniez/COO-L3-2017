package com.model;

public enum Symbole {
	VIDE(0),
	ROND(1),
	CROIX(2);
	
	
	private final int symbol;
	private Symbole (int symbol) {
		this.symbol = symbol;
	}

	// getter traditionnel
	public int getSymbol() {
		  return symbol;
	}
	
	public static Symbole fromInt(int text) {
      for (Symbole b : Symbole.values()) {
    	  if (text==b.symbol) {
    		  return b;
	      }
	  }
	  return null;
	}
}



