package com.model;

public enum Symbole {
	VIDE(0),
	ROND(1),
	CROIX(2);
	
	
	private final int symbol;
	private Symbole (int symbol) {
		this.symbol = symbol;
	}

	// getter traditionnel
	public int getSymbol() {
		  return symbol;
	}
	
	public static Symbole fromString(String text) {
	    if (text != null) {
	      for (Symbole b : Symbole.values()) {
	        if (text.equalsIgnoreCase(b.symbol)) {
	          return b;
	        }
	      }
	    }
	    return null;
	  }
}



